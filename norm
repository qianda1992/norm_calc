import numpy as np
import matplotlib.pyplot as plt
import pandas as pd


#a=np.array([[complex(1,-1),3],[2,complex(1,1)]])  
a = np.arange(12).reshape(4,3)
print(a)
print("矩阵的2范数")
print(np.linalg.norm(a,2) )   #计算矩阵2的范数
print("矩阵的1范数")
print(np.linalg.norm(a,1) )   #计算矩阵1的范数
print("矩阵的无穷范数")
print(np.linalg.norm(a,np.inf) )
print("矩阵的F范数")
print(np.linalg.norm(a,"fro") ) 


